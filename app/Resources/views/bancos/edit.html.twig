
<form name="appbundle_bancos" action="{{ path('bancos_edit', { 'id': banco.id }) }}" method="post" enctype="multipart/form-data">
    <div class="form-group">
        <label>Banco</label>
        {{ form_widget(edit_form.nombreBanco, { 'attr': {'class': 'form-control', 'placeholder':"Banco"} }) }}
    </div>

    <div class="form-group">
        <label>Foto</label>
        {{ form_widget(edit_form.foto_path) }}
    </div>

    {{ form_widget(edit_form._token) }}
    <button type="submit" class="btn btn-primary block full-width m-b">Actualizar</button>

</form>

